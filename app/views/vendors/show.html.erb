<div class="col-md-12">
  <div class="card">
    <div class="header">
      <h4 class="title">Thông Tin Nhà Cung Cấp</h4>
    </div>
    <div class="content">
      <div class="row">
        <div class="col-md-12">
          <div class="form-group">
            <%= label_tag 'Tên Nhà Cung Cấp' %><br>
            <p><%= @vendor.name %></p>
          </div>
        </div>
      </div>
      <%= link_to 'Sửa', edit_vendor_path(@vendor), class:"btn btn-info btn-fill pull-right", style:"margin-left: 10px" %>
      <%= link_to 'Thêm Nhà Cung Cấp', new_vendor_path, class:"btn btn-info btn-fill pull-right" %>
    <div class="clearfix"></div>
    </div>
  </div>
</div>